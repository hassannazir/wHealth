@model wHealth.User

@{
    ViewData["Title"] = "Edit";
}
<div class="row content-wrapper">
    <div class="col-md-4">
        <form asp-action="Edit" method="post">
            
            <input  asp-for="@Model.Id" class="form-control"/>
       
            <div class="form-group">
                <label asp-for="Status" class="control-label"></label>
                <input asp-for="Status" class="form-control" />
                <span asp-validation-for="Status" class="text-danger"></span>
            </div>
         
            <div class="form-group">
                <label asp-for="ClinicId" class="control-label"></label>
                <input asp-for="ClinicId" class="form-control" />
                <span asp-validation-for="ClinicId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" id="@Model.Id" formmethod="post" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>





